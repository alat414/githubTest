
# 1. #exercises:
# write a function that returns the max of two numbers.

def max_numbers():
    number1 = int(input("Enter the first numbers: "))
    number2 = int(input("Enter the second number: "))

    if number1 > number2:
        print(number1)
    elif number2 > number1:
        print(number2)
    else:
        print("Both Numbers are the same.")

def main():
    max_numbers()

main()
